FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      thermophysicalProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

thermoType
{
    type            heRhoThermo;
    mixture         pureMixture;
    transport       const;
    thermo          hConst;
    equationOfState perfectGas;
    specie          specie;
    energy          sensibleEnthalpy;
}

mixture
{
    specie
    {
        nMoles      1;
        molWeight   28.97;      // [g/mol]
    }
    thermodynamics
    {
        Cp          1007;       // [J/kg.K]
        Hf          0;
    }
    transport
    {
        mu          1.846e-05;  // [kg/m.s]
        Pr          0.707;
    }
}

