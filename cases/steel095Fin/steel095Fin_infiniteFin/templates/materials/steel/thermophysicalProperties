FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      thermophysicalProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

thermoType
{
    type            heSolidThermo;
    mixture         pureMixture;
    transport       constIso;
    thermo          eConst;
    equationOfState rhoConst;
    specie          specie;
    energy          sensibleInternalEnergy;
}

mixture
{
    specie
    {
        nMoles      1;
        molWeight   56.4;   // [g/mol]
    }
    transport
    {
        kappa       13.4;   // [W/m.K]
    }
    thermodynamics
    {
        Hf          0;
        Cv          468;    // [J/kg.K]
    }
    equationOfState
    {
        rho         8238;   // [kg/m^3]
    }
}

